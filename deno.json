{
    "name": "@esroyo/generic-pool",
    "version": "3.10.4",
    "exports": "./src/generic-pool.ts",
    "tasks": {
        "bump": "git tag bump && git push origin bump && git tag -d bump",
        "coverage": "deno task test --clean --reporter=dot --coverage=coverage && deno coverage --lcov --output=coverage/lcov.info --exclude='src/test-utils.ts|vendor' coverage && genhtml -o coverage/report coverage/lcov.info",
        "fmt": "deno fmt src/",
        "test": "deno test -A --parallel"
    },
    "fmt": {
        "indentWidth": 4,
        "singleQuote": true
    },
    "license": "MIT",
    "imports": {
        "@std/assert": "jsr:@std/assert@1.0.6"
    },
    "publish": {
        "exclude": [
            "**/*_test.ts",
            "**/*.test.ts",
            "**/test_*.ts",
            "**/*_bench.ts",
            "**/*.bench.ts",
            "**/*.snap",
            ".github/",
            "coverage/",
            "testdata/",
            "*.lcov",
            ".*"
        ]
    }
}
